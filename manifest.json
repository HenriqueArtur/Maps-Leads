{
  "manifest_version": 3,
  "name": "Google Maps Leads",
  "version": "1.0",
  "background": {
    "service_worker": "index.js"
  },
  "action": {
    "default_title": "Click to open panel"
  },
  "permissions": [
    "sidePanel",
    "tabs"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
}
